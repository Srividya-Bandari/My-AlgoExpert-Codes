   "source": [
    "def twoNumberSum(array, targetSum):\n",
    "    # Write your code here.\n",
    "\tHM = {}\n",
    "\tfor i in range (0, len(array)):\n",
    "\t\tD = targetSum - array[i]\n",
    "\t\tif D not in HM:\n",
    "\t\t\tHM[array[i]] = i\n",
    "\t\telse:\n",
    "\t\t\treturn [array[i], D]\n",
    "\treturn []\t\t\n",
    "\t\n",
    "    \n"
   ]
  }

   "source": [
    "def sortedSquaredArray(array):\n",
    "    # Write your code here.\n",
    "    outputarray = [0] * len(array)\n",
    "    for i in range(len(array)):\n",
    "        val = array[i]\n",
    "        outputarray[i] = val*val\n",
    "\n",
    "    outputarray.sort()\n",
    "\n",
    "    return outputarray\n",
    "        \n"
   ]
  }

    "def nonConstructibleChange(coins):\n",
    "    # Write your code here.\n",
    "    coins.sort()\n",
    "    change = 0\n",
    "    for coin in coins:\n",
    "        if coin > change + 1:\n",
    "            return change + 1\n",
    "        change += coin\n",
    "    return change + 1\n",
    "            \n",

    "def twoNumberSum(array, targetSum):\n",
    "    # Write your code here.\n",
    "\tfor i in range (0, len(array)-1):\n",
    "\t\tfor j in range (i+1, len(array)):\n",
    "\t\t\tif array[i] + array[j] == targetSum:\n",
    "\t\t\t\treturn [array[i], array[j]]\n",
    "\t\t\t\n",
    "\treturn []\n",
    "   \n"
   ]
  }

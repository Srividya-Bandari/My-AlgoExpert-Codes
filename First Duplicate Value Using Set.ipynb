   "source": [
    "def firstDuplicateValue(array):\n",
    "    # Write your code here.\n",
    "    Set = set()\n",
    "    for value in array:\n",
    "        if value in Set:\n",
    "            return value\n",
    "        Set.add(value)\n",
    "    return -1\n"
   ]
  }
